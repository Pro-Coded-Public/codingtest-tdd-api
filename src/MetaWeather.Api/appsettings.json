{
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ApiOptions": {
    "ApiUrl": "https://www.metaweather.com/",
    "Audience": "Metaweather.Api",
    "Authority": "https://localhost:5000",
    "MaxRecords": 5
  },
  "IdentityServer": {
    "Clients": {
      "MetaWeather.STS": {
        "Profile": "IdentityServerSPA"
      }
    }
  }
}